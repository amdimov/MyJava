    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String input = scanner.nextLine().toLowerCase();
        char[] inputChar = input.toCharArray();
        for (int i = 0; i < inputChar.length; i++) {
            char b = inputChar[i];
            tMeth(b);
        }

    }

