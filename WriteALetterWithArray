    public static void printer(int[][] alpabet){
        for (int i = 0; i < alpabet.length; i++) {
            for (int j = 3; j < alpabet[i].length; j++) {
                if (alpabet[i][j] != 0) {
                    System.out.print('*');
                } else {
                    System.out.print(" ");
                }
            }
            System.out.println();
        }
    }
